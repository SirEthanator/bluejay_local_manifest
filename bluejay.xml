<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote name="aosp-bluejay"
          fetch="https://android.googlesource.com"
          review="android-review.googlesource.com"
          revision="refs/tags/android-14.0.0_r73"
          clone-depth="1" />
  <remote name="sirethanator"
          fetch="https://github.com/SirEthanator/"
          revision="afterlife" />
  <remote name="lineage"
          fetch="https://github.com/LineageOS/"
          revision="lineage-21" />
  <remote name="muppets"
          fetch="https://github.com/themuppets"
          revision="lineage-21" />

  <remove-project path="device/google/gs-common" />

  <project path="device/google/bluejay" name="android_device_google_bluejay" remote="sirethanator" />
  <project path="device/google/bluejay-kernel" name="device/google/bluejay-kernel" remote="aosp-bluejay" />
  <project path="device/google/bluejay-sepolicy" name="device/google/bluejay-sepolicy" remote="aosp-bluejay" />
  <project path="device/google/gs101" name="android_device_google_gs101" remote="sirethanator" />
  <project path="device/google/gs101-sepolicy" name="device/google/gs101-sepolicy" remote="aosp-bluejay" />
  <project path="device/google/gs-common" name="android_device_google_gs-common" remote="lineage" />
  <project path="vendor/google/bluejay" name="proprietary_vendor_google_bluejay" remote="muppets" clone-depth="1" />
</manifest>

